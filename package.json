{
	"private": true,
	"name": "headmade-doc",
	"scripts": {
		"start": "dumi dev",
		"build": "dumi build",
		"clean": "rm -rf dist",
		"prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
		"deploy": "dumi build && cp CNAME dist/ && cp static/* dist && gh-pages -b main -o headmade-doc -d dist"
	},
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"typings": "dist/index.d.ts",
	"gitHooks": {
		"pre-commit": "lint-staged"
	},
	"lint-staged": {
		"*.{js,jsx,less,md,json}": [
			"prettier --write"
		],
		"*.ts?(x)": [
			"prettier --parser=typescript --write"
		]
	},
	"devDependencies": {
		"dumi": "^1.1.47",
		"gh-pages": "^4.0.0",
		"lint-staged": "^13.0.3",
		"prettier": "^2.7.1",
		"yorkie": "^2.0.0"
	},
	"dependencies": {}
}
